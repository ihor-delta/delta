  "description": {
    "data": "CI apps. In compliance with %freedoms%",
    "format": 1,
    "format-data": {
      "links": {
        {
          "name": "freedoms",
          "text": "Freedoms",
          "link": "https://github.com/ihor-delta/freedoms" 
        }
      }
    }
  },
  "mode" : 0,
  "improvisation-mode": {
    "global": 0,
    "exceptions": []
  },
  "schizophreny-mode" : {
    "global": 0,
    "exceptions": {}
  },
  "gods-mode": {
    "global":0,
    "exceptions": {}
  },
  "imagination-mode": {
    "global": 0,
    "exceptions": []
  },
  "informing-mode": {
    "global": 0,
    "exceptions": []
  },
  "collective-decision-mode": {
    "global": 0,
    "exceptions" : []
  },
  "delegation-mode": {
    "global": 0,
    "exceptions" : []
  },
  "diversification-mode": {
    "global": 0,
    "exceptions" : []
  },
  "freedom-of-will-mode": {
    "global": 0,
    "exceptions" : {
      "&IhorDelta": "https://github.com/ihor-delta/freedoms/blob/main/src/entities/IhorBudko.json"
    }
  },
  "competition-mode": {
    "global": 0,
    "exceptions" : []
  },
  "cooperation-mode": {
    "global":0,
    "exceptions":{
      "#TLKAE": ".apps.@TLKAE.config"
    }
  },
  "forget-mode": {
    "global":0,
    "exceptions": {}
  },
  "comparability-mode": {
    "global": 0,
    "exceptions" : []
  },
  "challenges-mode" : {
    "global": 0,
    "exceptions": []
  },
  "situative-authorities-mode":{0,
    "exceptions": {
      "TLKAE":".apps.@TLKAE"
    },
    "config": {
      "type":"config",
      "exclude":[".apps.Freedoms.Principles.situative-authorities.circumstances.country"],
      "anonimity-mode":1
    }
  },
  "apps":[
    "@Freedoms" : {
      "type": "app",
      "config" : {
        "id": "@Freeedoms"
        "type": "config",
        "mode": 1,
        "description" : {
          "data": "In compliance with Freedoms"
        }
      }
    },
    "@TLKAE": {
      "type":"app",
      "config": {
        "type": "config",
        "id": "@TLKAE@",
        "mode": 1,
        "situative-authorities-mode": 1,
        "description": "The Last King And Emperor" 
        "goal": "&The Venus Project& by &Roxane Meadows& and &JacqueFresco& on Earth"
      }
    },
    "@Insights": {
      "type":"app",
      "config": {
        "id": "@Insights@",
        "type":"config",
        "mode": {
          "global": 0,
          "exceptions": {
            "#IhorDelta" :"https://github.com/ihor-delta/freedoms/blob/main/src/entities/IhorBudko.json"
          }
        }
      }
    },
    "@Economy": {
      "type": "app",
      "config": {
        "type":"config",
        "id": "@Economy@",
        "mode":{
          "global":1,
          "exceptions": {
            "Ihor.Delta":"https://github.com/ihor-delta/freedoms/blob/main/src/entities/IhorBudko.json",
          }
        },
        "cooperation-mode": {
          "global": 1,
          "exceptions": {}
        },
        "exclude": ["stealing"],
        "apps": {
          "@MonetarySystem": {
              "type": "app",
              "config":{
                "id": "@MonetarySystem@",
                "type":"config",
                "mode": {
                  "global": 0,
                  "exceptions" :{
                    "work-evaluating" : 1
                  }
                }
              }
           },
           "@ResourceBasedEconomy":{
             "type": "app",
             "config": {
               "id":"@ResourceBasedEconomy@",
               "type": "config",
               "mode": {
                 "global": 1,
                 "exceptions" : {
                   
                 } 
               }
             },
             "link": "https://thevenusproject.com"
           }
        }
      }
    },
    "@Obraz": {
      "type":"app",
      "config" : {
        "id": "@Obraz@",
        "type":"config",
        "situative-authorities-mode": 1,
        "mode": {
          "global": 1,
          "exceptions": []
        },
      },
      "link": "https://obraz.io/"
    },
    "@Reputation" : {
      "type":"app",
      "id": "@Reputation@",
      "config": {
        "type": "config",
        "improvisation-mode" : 1,
        "mode" : {
          "global": 0, 
          "exceptions": {
            "The.Venus.Project": "&The Venus Project& by &Roxane Meadows& and &Jacque Fresco&"
          }
        }
      }
    },
    "@WarsFinish" : {
      "type":"app",
      "id":"@WarsFinish@",
      "priority": "important/urgent",
      "config" : {
        "type":"config",
        "imprivisation-mode": {
          "type":"config",
          "mode":0
        },
        "diversification-mode": 1,
        "delegation-mode":1,
        "mode": {
          "global": 1,
          "exceptions": {}
        },
        "conditions": [
          "Diplomatic way",
          "Wars officially finished"
        ]
      }
    },
    "@SharingIdeasAndCollaboration": {
      "type":"app", 
      "config": {
        "slow-mode": 1,
        "priorities": ["powerty", "politics","war", "religion"],
        "mode":0,
        "goal": "&The Venus Project& by &Roxane Meadows& and &Jacque Fresco&",
        "filters": {
          "TLKAE": ".apps.@TLKAE"
          "TheVenusProject": {
            "type":"entity",
            "id": "The Venus Project"
            "config": {
              "type":"config",
              "name": "The Venus Project",
              "address" : "Florida, USA",
              "links": [
                "https://thevenusproject.com"
              ]
            }
          }
        }
      }
    }
  ]
}
