  "name": "World",
  "description": {
    "data": "In compliance with %freedoms%",
    "format": 1,
    "format-data": {
      "links": {
        {
          "name": "freedoms",
          "text": "Freedoms",
          "link": "https://github.com/ihor-delta/freedoms" 
        }
      }
    }
  },
  "config": {
    "mode" : 0,
    "imitation-mode": 0,
    "communication-mode": 0,
    "countries-mode": {
      "global": 0,
      "exceptions": {}
    },
    "languages-mode": 0,
    "development-mode": {
      "global": 0,
      "exceptions": {
        "Ihor-Delta": "https://github.com/ihor-delta/freedoms/blob/main/src/entities/IhorBudko.json"
      }
    },
    "permissions-mode": {
      "global": 1,
      "exceptions": {},
      "default": "everybody"
    },
    "modeeling-situations-mode": 0,
    "matrix-mode": {
      "global": 0,
      "exceptions": {}
    },
    "anonimity-mode": {
      "global": 0,
      "exceptions": {}
    },
    "nationalities-mode": {
      "global": 0,
      "exceptions": {}
    },
    "borders-mode": {
      "global": 0,
      "exceptions": {}
    },
    "improvisation-mode": {
      "global": 0,
      "exceptions": {}
    },
    "schizophreny-mode" : {
      "global": 0,
      "exceptions": {}
    },
    "gods-mode": {
      "global":0,
      "exceptions": {}
    },
    "imagination-mode": {
      "global": 0,
      "exceptions": {}
    },
    "informing-mode": {
      "global": 0,
      "exceptions": {}
    },
    "collective-decision-mode": {
      "global": 0,
      "exceptions" : {}
    },
    "delegation-mode": {
      "global": 0,
      "exceptions" : {}
    },
    "diversification-mode": {
      "global": 0,
      "exceptions" : {}
    },
    "freedom-of-will-mode": {
      "global": 0,
      "exceptions" : {
        "Ihor-Delta": "https://github.com/ihor-delta/freedoms/blob/main/src/entities/IhorBudko.json"
      }
    },
    "competition-mode": {
      "global": 0,
      "exceptions" : {}
    },
    "cooperation-mode": {
      "global":0,
      "exceptions":{
        "TLKAE": ".apps.@TLKAE.config"
      }
    },
    "forget-mode": {
      "global":0,
      "exceptions": {}
    },
    "comparability-mode": {
      "global": 0,
      "exceptions" : {}
    },
    "challenges-mode" : {
      "global": 0,
      "exceptions": {}
    },
    "situative-authorities-mode":{
      "mode": 0,
      "exceptions": {
        "TLKAE":".apps.@TLKAE"
      },
      "config": {
        "type":"config",
        "exclude":[".apps.@Freedoms.Principles.situative-authorities.circumstances.country"],
        "anonimity-mode": 1
      }
    },
    "geopolitics-mode": 0,
  }
  "apps":{
    "@ChallengesAggregator": {
      "type": "app",
      "config": {
        "type": "config",
        "goal": "Make challenges for representatives"
        "filters": {
          "@PlanningAggregator": ".apps.@ChallengesAggregator",
        },
      }
    },
    "@PlanningAggregator": {
      "type": "app",
      "config": {
        "situative-authorities-mode": 1,
        "id": "@PlanningAggregator@",
        "type": "config",
        "goal": "Planning of operations",
        "filters": {
          "China": {
            "type": "entity",
            "config": {
              "category" : "countries",
              "name" : "China",
              "id": "&China&"
            }
          }
        }
      }
    },
    "@TheVenusProjectAggregator" : {
      "type": "app",
      "config": {
        "id": "@TheVenusProjectAggregator@",
        "type": "config",
        "goal": "Realisation of &The Venus Project& on Earth and reforming culture",
        "description": "Aggregator of information for &The Venus Project&",
        "permissions": {
          "TVP": "&The Venus Project&"
        },
      },
      "description": "&The Venus Project& Monitor"
    },
    "@Statistics": {
      "type": "app",
      "config": {
        "slow-mode": 1,
        "id": "@Statistics@",
        "type": "config",
        "mode": 1,
        "goal": "Getting statistics all over the world",
        "aggregators": {
          "TheVenusProject": ".apps.@TheVenusProjectAggregator"
        },
        "filters": {
          "TheVenusProject": {
            "type" : "entity",
            "config": {
              "id": "&The Venus Project&",
              "address": "Florida, USA",
              "type": "config",
              "authors": {
                "Roxane Meadows" : {
                  "id": "&Roxane Meadows&",
                  "type" : "entity"
                  "config": {
                    "id" : "&Roxane&",
                    "name": "Roxane Meadows",
                    "address": "Florida, USA"
                  }
                },
                "Jacque-Fresco" : {
                  "id": "&Jacque Fresco&",
                  "type" : "entity"
                  "config": {
                    "id" : "&Jacque&",
                    "name": "Jacque Fresco"
                  }
                }
              }
            }
          }
        }
      }
    },
    "@Freedoms" : {
      "type": "app",
      "config" : {
        "id": "@Freeedoms@"
        "type": "config",
        "mode": 1,
        "informing-mode": 1,
        "description" : {
          "data": "In compliance with &Freedoms&"
        },
        "apply": "mankind"
      }
    },
    "@TLKAE": {
      "type":"app",
      "config": {
        "type": "config",
        "id": "@TLKAE@",
        "mode": 1,
        "situative-authorities-mode": 1,
        "description": "The Last King And Emperor" 
        "goal": "&The Venus Project& by &Roxane Meadows& and &JacqueFresco& on Earth"
      }
    },
    "@Insights": {
      "type":"app",
      "config": {
        "id": "@Insights@",
        "type":"config",
        "mode": {
          "global": 0,
          "exceptions": {
            "Ihor-Delta" :"https://github.com/ihor-delta/freedoms/blob/main/src/entities/IhorBudko.json"
            "The-Venus-Project": "&The Venus Project&"
          }
        }
      }
    },
    "@SocialGames": {
      "type": "app",
      "config": {
        "id": "@SocialGames@",
        "type":"config",
        "mode": {
          "global": 1,
          "exceptions": {
            "The-Venus-Project" : "&The-Venus-Project&" 
          }
        },
        "goal": "Resolve political and geopolitical conflicts",
        "countries-mode":1,
        "collective-decision-mode": {
          "mode": 1,
          "exceptions": {},
          "rules": {
            "elections": 0
          }
        },
        "situative-authorities-mode":1,
        "filters": {
          "Denmark": {
            "type": "entity",
            "config": {
              "category": "countries",
              "id":"&Denmark&",
              "name":"Denmark"
            }
          },
          "Finland": {
            "type": "entity",
            "config": {
              "category": "countries",
              "id":"&Finland&",
              "name":"Finland"
            }
          },
          "Iceland": {
            "type": "entity",
            "config": {
              "category": "countries",
              "id":"&Iceland&",
              "name":"Iceland"
            }
          },
          "Switzerland": {
            "type": "entity",
            "config": {
              "category": "countries",
              "id":"&Switzerland&",
              "name":"Switzerland"
            }
          },
          "Norway": {
            "type": "entity",
            "config": {
              "category": "countries",
              "id":"&Norway&",
              "name":"Norway"
            }
          }
        },
        "apps": {
          "@Sociocybernetics": {
            "type": "app",
            "config": {
              "type": "config",
              "id": "@Socyocybernetics@",
              "goal" : "Provide sociocybernetics instead of political systems"
              "apps" : {
                "@Politics": {
                  "type": "app",
                  "config": {
                    "goal": "Migration of politics to sociocybernetics",
                    "countries-mode": 1,
                    "slow-mode": 1,
                    "situative-authorities-mode": 1,
                    "id": "@Politics@",
                    "filters": {
                      "Canada": {
                        "type": "entity",
                        "config": {
                          "category": "countries",
                          "name": "Canada",
                          "id": "&Canada&"
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "@Economy": {
      "type": "app",
      "config": {
        "type":"config",
        "id": "@Economy@",
        "mode":{
          "global":1,
          "exceptions": {
            "Ihor-Delta":"https://github.com/ihor-delta/freedoms/blob/main/src/entities/IhorBudko.json",
          }
        },
        "cooperation-mode": {
          "global": 1,
          "exceptions": {}
        },
        "exclude": ["stealing"],
        "apps": {
          "@MonetarySystem": {
              "type": "app",
              "config":{
                "id": "@MonetarySystem@",
                "type":"config",
                "mode": {
                  "global": 0,
                  "exceptions" :{
                    "work-evaluating" : 1
                  }
                }
              }
           },
           "@ResourceBasedEconomy":{
             "type": "app",
             "config": {
               "id":"@ResourceBasedEconomy@",
               "type": "config",
               "mode": {
                 "global": 1,
                 "exceptions" : {
                   
                 } 
               }
             },
             "link": "https://thevenusproject.com"
           }
        }
      }
    },
    "@Obraz": {
      "type":"app",
      "config" : {
        "id": "@Obraz@",
        "type":"config",
        "situative-authorities-mode": 1,
        "mode": {
          "global": 1,
          "exceptions": []
        },
      },
      "link": "https://obraz.io/"
    },
    "@Reputation" : {
      "type":"app",
      "id": "@Reputation@",
      "config": {
        "type": "config",
        "improvisation-mode" : 0,
        "mode" : {
          "global": 0, 
          "exceptions": {
            "The-Venus-Project": "&The Venus Project& by &Roxane Meadows& and &Jacque Fresco&"
          }
        }
      }
    },
    "@WarsFinish" : {
      "type":"app",
      "id":"@WarsFinish@",
      "priority": "important/urgent",
      "config" : {
        "type":"config",
        "imprivisation-mode": {
          "type":"config",
          "mode":0
        },
        "diversification-mode": 1,
        "delegation-mode":1,
        "mode": {
          "global": 1,
          "exceptions": {}
        },
        "conditions": [
          "Diplomatic way",
          "Wars officially finished"
        ]
      }
    },
    "@SharingIdeasAndCollaboration": {
      "type":"app", 
      "config": {
        "slow-mode": 1,
        "priorities": ["powerty", "politics","war", "religions"],
        "mode":1,
        "goal": "&The Venus Project& by &Roxane Meadows& and &Jacque Fresco&",
        "filters": {
          "Obraz": ".apps.@Obraz",
          "TLKAE": ".apps.@TLKAE"
          "The-Venus-Project": {
            "type":"entity",
            "config": {
              "category": "organisations",
              "id": "The Venus Project",
              "type":"config",
              "name": "The Venus Project",
              "address" : "Florida, USA",
              "links": [
                "https://thevenusproject.com"
              ]
            }
          }
        }
      }
    },
    "@Alert": {
      "type": "app",
      "config": {
        "informing-mode": 1,
        "id": "@Alert@",
        "mode": 0,
        "cooperation-mode": 1,
        "permissions": [
          {
            "type": "entity",
            "query": {
              "individual" : {
                "profession": "policeman"
              }
            }
          },
          {
            "type": "entity",
            "query": {
              "individual": {
                "state": "victim"
              }
            },
            "sort": {
              "parameter": "age-group",
              "direction" : "ASC"
            }
          }
        ]
      }
    }
  }
}
